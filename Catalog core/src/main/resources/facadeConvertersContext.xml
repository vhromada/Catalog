<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="coreConversionService" class="org.springframework.context.support.ConversionServiceFactoryBean">
		<property name="converters">
			<set>
				<ref bean="movieToMovieTOConverter"/>
				<ref bean="movieTOToMovieConverter"/>
				<ref bean="mediumToIntegerConverter"/>
				<ref bean="integerToMediumConverter"/>
				<ref bean="serieToSerieTOConverter"/>
				<ref bean="serieTOToSerieConverter"/>
				<ref bean="seasonToSeasonTOConverter"/>
				<ref bean="seasonTOToSeasonConverter"/>
				<ref bean="episodeToEpisodeTOConverter"/>
				<ref bean="episodeTOToEpisodeConverter"/>
				<ref bean="gameToGameTOConverter"/>
				<ref bean="gameTOToGameConverter"/>
				<ref bean="musicToMusicTOConverter"/>
				<ref bean="musicTOToMusicConverter"/>
				<ref bean="songToSongTOConverter"/>
				<ref bean="songTOToSongConverter"/>
				<ref bean="programToProgramTOConverter"/>
				<ref bean="programTOToProgramConverter"/>
				<ref bean="bookCategoryToBookCategoryTOConverter"/>
				<ref bean="bookCategoryTOToBookCategoryConverter"/>
				<ref bean="bookToBookTOConverter"/>
				<ref bean="bookTOToBookConverter"/>
				<ref bean="genreToGenreTOConverter"/>
				<ref bean="genreTOToGenreConverter"/>
			</set>
		</property>
	</bean>

</beans>
